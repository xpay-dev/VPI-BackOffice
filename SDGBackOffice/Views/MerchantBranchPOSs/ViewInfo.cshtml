@{
    ViewBag.Title = "POS ViewMyInfo";
}

@model SDGBackOffice.Models.MerchantPOSModel
<nav class="top-menu" ng-class="{'hidden-top-menu': hideTopMenu}">
    <div class="col-sm-6">        <!-- START BREADCRUMB -->
        <ul class="breadcrumb">
            <li><a href="@Url.Action("Index", "MerchantBranchPOSs")">Home</a></li>
            <li><a href="#">POS Management</a></li>
            <li class="active">View POS Info</li>
        </ul>
        <!-- END BREADCRUMB -->
    </div>
    <div class="col-sm-6">
        @Html.Partial("~/Views/Shared/_PartialUser.cshtml")
    </div>
</nav>
<section class="page-content">
    <div class="page-content-inner">
        <section class="panel">
            <div class="panel-heading">
                <h3>POS Details</h3>
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="margin-bottom-50">
                            <!-- Horizontal Form -->
                            <form class="form-horizontal">
                                <div class="form-group row">
                                    <div class="col-md-4">
                                        <label class="form-control-label">POS Name</label>
                                        @Html.TextBoxFor(m => m.POSName, new { @class = "form-control", @placeholder = "POS Name", @disabled = "disabled" })
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <div class="col-md-4">
                                        <label class="form-control-label">Status</label>
                                        @Html.TextBoxFor(m => m.POSStatus, new { @class = "form-control", @placeholder = "Status", @disabled = "disabled" })
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <div class="col-md-4">
                                        <label class="form-control-label">Branch Name</label>
                                        @Html.TextBoxFor(m => m.BranchName, new { @class = "form-control", @placeholder = "Branch Name", @disabled = "disabled" })
                                    </div>
                                </div>

                                <div class="form-actions">
                                    <div class="form-group row">
                                        <div class="col-md-3 pull-left">
                                            @Html.ActionLink("Back", "Index", "MerchantBranchPOSs", null, new { @class = "btn btn-default" })
                                        </div>
                                        <div class="col-md-9 ">
                                        </div>
                                    </div>
                                </div>
                            </form>
                            <!-- End Horizontal Form -->
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
</section>

@section scripts {
    <script type="text/javascript">
        $("input, select").css("color", "black");
    </script>
}