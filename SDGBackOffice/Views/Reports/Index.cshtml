            });
            $("#view").click(function () {
                var uType = '@(ViewBag.UserType)'
                var user;

                var start = document.getElementById('datepickerstart').value;
                var end = document.getElementById('datepickerend').value;
                var r = document.getElementById('ddlresellers');
                var m = document.getElementById('ddlmerchants');

                if (r != null) {
                    r = document.getElementById('ddlresellers').value;
                } else { r = 0; }

                if (m != null) {
                    m = document.getElementById('ddlmerchants').value;
                } else { m = 0; }

                var b = document.getElementById('ddlbranches').value;
                var mp = document.getElementById('ddlpos').value;
                var type = document.getElementById('ddltranstype').value;
                var action = document.getElementById('ddlactions').value;

                if (start == "" && end == "") {
                    start = "1/1/0001";
                    end = "1/1/0001";
                } else if (start == "") {
                    start = "1/1/0001";
                } else if (end == "") {
                    end = "1/1/0001";
                } else if (start != "" && end != "") {
                    if ((new Date(start).getTime() > new Date(end).getTime())) {
                        $("#errMsg").text("Check your Date Filtering. End date is less than Start date.");
                        $("#message-box-danger").show();
                    }
                }

                if (r > 0) {
                    if (type > 0) {
                        if (type == 4) {
                            $("#tbl1").hide();
                            refreshDebitReportsTable(0, r, m, mp, b, type, action, start, end);
                            $("#tbl2").show();
                        } else if (type == 5) {
                            $("#tbl2").hide();
                            $("#tbl1").show();
                            refreshReportsTable(0, r, m, mp, b, type, action, start, end);
                        } else {
                            $("#tbl2").hide();
                            $("#tbl1").show();
                            refreshReportsTable(0, r, m, mp, b, type, action, start, end);
                        }
                    } else {
                        if (uType == "reseller") {
                            document.getElementById('hdnType').value = uType;
                            document.getElementById('userType').value = "reseller";
                            $("#tbl2").hide();
                            $("#tbl1").show();
                            refreshReportsTable(0, pId, m, mp, b, type, action, start, end);
                        } else if (uType == "merchant") {
                            document.getElementById('hdnType').value = uType;
                            document.getElementById('userType').value = "merchant";
                            $("#tbl2").hide();
                            $("#tbl1").show();
                            refreshReportsTable(0, 0, pId, mp, b, type, action, start, end);
                        } else {
                            $("#tbl2").hide();
                            $("#tbl1").show();
                            refreshReportsTable(pId, r, m, mp, b, type, action, start, end);
                        }
                        //$("#errMsg").text("Please select a Transaction Type.");
                        //$("#message-box-danger").show();
                    }
                } else if (r == 0) {
                    if (type > 0) {
                        if (type == 4) {
                            $("#tbl1").hide();
                            refreshDebitReportsTable(pId, r, m, mp, b, type, action, start, end);
                            $("#tbl2").show();
                        } else {
                            if (uType == "reseller") {
                                document.getElementById('hdnType').value = uType;
                                document.getElementById('userType').value = "reseller";
                                $("#tbl2").hide();
                                $("#tbl1").show();
                                refreshReportsTable(0, pId, m, mp, b, type, action, start, end);
                            } else if (uType == "merchant") {
                                document.getElementById('hdnType').value = uType;
                                document.getElementById('userType').value = "merchant";
                                $("#tbl2").hide();
                                $("#tbl1").show();
                                refreshReportsTable(0, 0, pId, mp, b, type, action, start, end);
                            } else {
                                $("#tbl2").hide();
                                $("#tbl1").show();
                                refreshReportsTable(pId, r, m, mp, b, type, action, start, end);
                            }
                        }
                    } else {
                        if (uType == "reseller") {
                            document.getElementById('hdnType').value = uType;
                            document.getElementById('userType').value = "reseller";
                            $("#tbl2").hide();
                            $("#tbl1").show();
                            refreshReportsTable(0, pId, m, mp, b, type, action, start, end);
                        } else if (uType == "merchant") {
                            document.getElementById('hdnType').value = uType;
                            document.getElementById('userType').value = "merchant";
                            $("#tbl2").hide();
                            $("#tbl1").show();
                            refreshReportsTable(0, 0, pId, mp, b, type, action, start, end);
                        } else {
                            $("#tbl2").hide();
                            $("#tbl1").show();
                            refreshReportsTable(pId, r, m, mp, b, type, action, start, end);
                        }
                        //$("#errMsg").text("Please select a Transaction Type.");
                        //$("#message-box-danger").show();
                    }
                } else {
                    $("#errMsg").text("Please select a Partner");
                    $("#message-box-danger").show();
                }
            });

            $("#close").click(function () {
                $("#message-box-danger").hide();
            });

            $("#xMap").click(function () {
                $('.3rdMap').hide();
                $("html, body").animate({ scrollTop: 0 }, 250);

            });

            $("#x").click(function () {
                $(".2nd").hide();
                $(".1st").show();
                $('.3rdMap').hide();
