@{
    ViewBag.Title = "Result";
    Layout = Request.IsAjaxRequest() ? null : "~/Views/Shared/_Layout.cshtml";
}
<nav class="top-menu" ng-class="{'hidden-top-menu': hideTopMenu}">
    <div class="col-sm-6">        <!-- START BREADCRUMB -->
        <ul class="breadcrumb">
            <li><a href="#">Home</a></li>
            <li class="active">Merchant On Board</li>
        </ul>
    </div>
    <div class="col-sm-6">
        @Html.Partial("~/Views/Shared/_PartialUser.cshtml")
    </div>
</nav>

<section class="page-content">
    <div class="page-content-inner">
        <section class="panel">
            <div class="panel-heading">
                <h4>Result</h4>
            </div>
            <div class="panel-body">
                <div class="form-group row">
                    <div class="col-md-6">
                        <h4>Your request has been successfully sent. Please wait while your request is being processed. To get the results, Go to <span style="font-weight:bold">Merchant On Board -> Get FileNames</span></h4> 
                    </div>
                </div>

                <div class="form-actions">
                    <div class="form-group row">
                        <div class="col-md-3 pull-left">
                            @Html.ActionLink("Back", "Add", "MerchantOnBoard", null, new { @class = "btn btn-default" })
                        </div>
                        <div class="col-md-9 ">

                        </div>
                    </div>
                </div>
            </div>

        </section>
        <!-- End  -->
    </div>
</section>