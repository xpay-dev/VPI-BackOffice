@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- START BREADCRUMB -->
<ul class="breadcrumb">
    <li><a href="#">Home</a></li>
    <li class="active">Audit Trail</li>
</ul>

<div class="swal-btn-success"></div>
<div class="swal-btn-cancel"></div>

<section class="page-content">
    <div class="page-content-inner">
        @Html.Hidden("success", (object)ViewBag.Success)
        <section class="panel">
            <div class="panel-heading">
                <h3>View All Audit Trails</h3>
            </div>

            <div class="panel-body">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="margin-bottom-50">
                            <table class="table table-inverse nowrap" id="auditTrailTbl" width="100%">
                                <thead>
                                    <tr>
                                        <th>Date Last Log in</th>
                                        <th>Parent Partner</th>
                                        <th>Name</th>
                                        <th>IP Adress</th>
                                        <th>Status</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- End  -->
    </div>
</section>

@section scripts {

    <!-- END PAGE PLUGINS -->

    <script type="text/javascript">
        $(document).ready(function () {

            $('#example1').DataTable({
                responsive: true
            });

            $("#auditTrailMenu").addClass("active");
            $(".page-content").css("height", "100%");

            var strSuccess = document.getElementById('success').value;
            if (strSuccess != "") {
                getModal(0, strSuccess);
            }

            refreshAuditTrailTable();

        });
    </script>
}




